			<Visibility>Expert</Visibility>
			<!--<pIsLocked>TLParamsLocked</pIsLocked>-->
			<pIsLocked>IsImageFormatLocked</pIsLocked>
			<Streamable>Yes</Streamable>
			<pValue>pLinePitchReg</pValue>
			<pMin>pLinePitchMin</pMin>
			<pMax>pLinePitchMax</pMax>
			<!--<Inc></Inc>-->
			<Representation>Linear</Representation>
		</Integer>
		<IntReg Name="pLinePitchReg">
			<Address>0x20074</Address>
			<Length>4</Length>
			<AccessMode>RW</AccessMode>
			<pPort>Device</pPort>
			<Cachable>WriteAround</Cachable>
			<pInvalidator>pWidthReg</pInvalidator>
			<pInvalidator>pHeightReg</pInvalidator>
			<pInvalidator>pOffsetXReg</pInvalidator>
			<pInvalidator>pOffsetYReg</pInvalidator>
			<pInvalidator>pBinningHorizontalReg</pInvalidator>
			<pInvalidator>pPixelFormatReg</pInvalidator>
	  <pInvalidator>pUserSetLoadReg</pInvalidator>
	  <!--<pInvalidator>pPixelCodingReg</pInvalidator>-->
	  <pInvalidator>pPixelSizeReg</pInvalidator>
	  <Sign>Unsigned</Sign>
	  <Endianess>BigEndian</Endianess>
	</IntReg>
	<IntSwissKnife Name="pLinePitchMin">
	  <pVariable Name="INDEX">pPixelSizeReg</pVariable>
	  <Formula>
		(INDEX=0x00080000) ? 32 : (
		(INDEX=0x000C0000) ? 24 : (
		(INDEX=0x00100000) ? 32 : 24 ))
	  </Formula>
	</IntSwissKnife>
	<IntSwissKnife Name="pLinePitchMax">
	  <pVariable Name="INDEX">pPixelSizeReg</pVariable>
	  <pVariable Name="MAXW">pWidthMaxReg</pVariable>
	  <Formula>
		(INDEX=0x00080000) ? MAXW*1 : (
		(INDEX=0x000C0000) ? MAXW + MAXW/2 : (
		(INDEX=0x00100000) ? MAXW*2 : MAXW*3 ))
	  </Formula>
	</IntSwissKnife>

