	<IntSwissKnife Name="PacketPayload" NameSpace="Custom">
	  <ToolTip>This is the amount of payload data that is sent in each GVSP image packet.</ToolTip>
	  <Description>This is the amount of payload data that is sent in each GVSP image packet.</Description>
	  <DisplayName>Image Packet Payload</DisplayName>
	  <Visibility>Guru</Visibility>
	  <Streamable>No</Streamable>
	  <pVariable Name="PACKETSIZE">GevSCPSPacketSize</pVariable>
	  <Formula>PACKETSIZE - 36</Formula>
	</IntSwissKnife>

	<IntSwissKnife Name="NumberOfPacketsPerImage" NameSpace="Custom">
	  <ToolTip>Number of individual packets per image.</ToolTip>
	  <Description>Intermediate value. This value is calculated based on the Payload Size and the Packet Size. This is the number of individual packets each image will be broken into when it is sent over the Stream channel</Description>
	  <DisplayName>Number of Packets</DisplayName>
	  <Visibility>Guru</Visibility>
	  <Streamable>No</Streamable>
	  <pVariable Name="PACKETPAYLOAD">PacketPayload</pVariable>
	  <pVariable Name="PAYLOADSIZE">PayloadSize</pVariable>
	  <Formula>(((PAYLOADSIZE/PACKETPAYLOAD)+2)%PACKETPAYLOAD=0)?((PAYLOADSIZE/PACKETPAYLOAD)+2):((PAYLOADSIZE/PACKETPAYLOAD)+3)</Formula>
	</IntSwissKnife>

	<IntSwissKnife Name="TotalImageSizeOnNetwork" NameSpace="Custom">
	  <ToolTip>Total number of bytes sent per image including GVSP overhead.</ToolTip>
	  <Description>Total number of bytes sent per image including GVSP overhead.</Description>
	  <DisplayName>Total Image Size (Payload + GVSP overhead)</DisplayName>
	  <Visibility>Guru</Visibility>
	  <Streamable>No</Streamable>
	  <pVariable Name="PAYLOADSIZE">PayloadSize</pVariable>
	  <pVariable Name="PACKETCOUNT">NumberOfPacketsPerImage</pVariable>
	  <Formula>PAYLOADSIZE+(PACKETCOUNT*54)+36+12</Formula>
	</IntSwissKnife>

	<SwissKnife Name="TotalImageTransmissionTimePerSecond" NameSpace="Custom">
	  <ToolTip>Total Image Transmission Time per second</ToolTip>
	  <Description>Total amount of time it takes to send the image data per second at the specified frame-rate</Description>
	  <DisplayName>Total Image Transmission Time per second (s)</DisplayName>
	  <Visibility>Guru</Visibility>
	  <Streamable>No</Streamable>
	  <pVariable Name="MAXFRAMERATE">MaxAcquisitionFrameRate</pVariable>
	  <pVariable Name="TOTALSIZE">TotalImageSizeOnNetwork</pVariable>
	  <Formula>(MAXFRAMERATE * TOTALSIZE)/(1000000000.0 / 8.0)</Formula>
	</SwissKnife>

	<SwissKnife Name="TotalPauseTimePerSecond" NameSpace="Custom">
	  <ToolTip>Total pause time to be added to the image data transmission per second</ToolTip>
	  <Description>Total pause time to be added to the image data transmission per second</Description>
	  <DisplayName>Total Pause Time (s)</DisplayName>
	  <Visibility>Guru</Visibility>
	  <Streamable>No</Streamable>
	  <pVariable Name="EXPECTEDBANDWIDTHUSAGE">ExpectedBandwidthUsage</pVariable>
	  <pVariable Name="TRANSMISSIONTIME">TotalImageTransmissionTimePerSecond</pVariable>
	  <Formula>(EXPECTEDBANDWIDTHUSAGE / 100.0) - TRANSMISSIONTIME</Formula>
	</SwissKnife>

	<SwissKnife Name="InterPacketDelayTime" NameSpace="Custom">
	  <ToolTip>Inter-Packet Delay time in seconds</ToolTip>
	  <Description>Inter-Packet Delay time (in seconds) to be added between each GVSP image packet in order to achieve the specified maximum frame-rate</Description>
	  <DisplayName>Inter-Packet Delay Time (s)</DisplayName>
	  <Visibility>Guru</Visibility>
	  <Streamable>No</Streamable>
	  <pVariable Name="PAUSETIME">TotalPauseTimePerSecond</pVariable>
	  <pVariable Name="NUMBEROFPACKETS">NumberOfPacketsPerImage</pVariable>
	  <pVariable Name="MAXFRAMERATE">MaxAcquisitionFrameRate</pVariable>
	  <Formula>PAUSETIME / (NUMBEROFPACKETS * MAXFRAMERATE)</Formula>
	</SwissKnife>

	<SwissKnife Name="InterPacketDelayEstimate" NameSpace="Custom">
	  <ToolTip>Estimate for the Inter-Packet Delay value to be used with this camera</ToolTip>
	  <Description>
		This is an estimate of the Inter-Packet Delay value to be used with this camera. This value has to be thought of as a starting point for finding the optimal Inter-Packet Delay value for the camera when it is used in a multi-camera system.
		The calculation is based on the assumption that if the stream channel communication is spread over time as much as possible using the Inter-Packet Delay - but only so much that the maximum specified frame rate still can be achieved, then the camera will have the minimal impact on the communication from the other cameras connected on the same Gigabit Ethernet network.
		A little overhead should always be reserved for retransmissions for the image stream so it is not adviced to use 100% as the Expected Bandwidth usage. A typical value would be around 90-95%
	  </Description>
	  <DisplayName>Inter-Packet Delay Estimate</DisplayName>
	  <Visibility>Expert</Visibility>
	  <Streamable>No</Streamable>
	  <pVariable Name="PACKETDELAYTIME">InterPacketDelayTime</pVariable>
	  <pVariable Name="GEVTIMESTAMPTICKFREQUENCY">GevTimestampTickFrequency</pVariable>
	  <Formula>TRUNC(PACKETDELAYTIME*GEVTIMESTAMPTICKFREQUENCY+0.5)</Formula>
	</SwissKnife>

	<Float Name="ExpectedBandwidthUsage" NameSpace="Custom">
	  <ToolTip>Expected bandwidth usage in percent of a 1 Gbit/s connection</ToolTip>
	  <Description>Expected bandwidth usage in percent of a 1 Gbit/s connection</Description>
	  <DisplayName>Expected Bandwidth Usage (%)</DisplayName>
	  <Visibility>Expert</Visibility>
	  <Streamable>No</Streamable>
	  <Value>90.0</Value>
	  <Min>0.0</Min>
	  <Max>100.0</Max>
	</Float>

	<Float Name="MaxAcquisitionFrameRate" NameSpace="Custom">
	  <ToolTip>Expected maximum number of images sent per second.</ToolTip>
	  <Description>Expected maximum number of images sent per second. This will typically be the frame-rate when the camera is running continuous</Description>
	  <DisplayName>Maximum Acquisition Frame-rate (fps)</DisplayName>
	  <Visibility>Expert</Visibility>
	  <Streamable>No</Streamable>
	  <Value>20.8</Value>
	  <Min>0.33</Min>
	  <Max>59.94</Max>
	</Float>
  </Group>

