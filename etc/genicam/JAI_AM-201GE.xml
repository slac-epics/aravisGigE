	  <pVariable Name="W">GevSupportedOptionalCommandsWRITEMEMReg</pVariable>
	  <pVariable Name="PR">GevSupportedOptionalCommandsPACKETRESENDReg</pVariable>
	  <pVariable Name="E">GevSupportedOptionalCommandsEVENTReg</pVariable>
	  <pVariable Name="ED">GevSupportedOptionalCommandsEVENTDATAReg</pVariable>
	  <pVariable Name="P">GevSupportedOptionalCommandsPendingAckReg</pVariable>
	  <pVariable Name="A">GevSupportedOptionalCommandsActionReg</pVariable>
	  <pVariable Name="ES">GevSupportedOptionalCommandsExtStatReg</pVariable>
	  <pVariable Name="DD">GevSupportedOptionalCommandsDiscDelReg</pVariable>
	  <pVariable Name="WD">GevSupportedOptionalCommandsWriteDiscDelReg</pVariable>
	  <pVariable Name="TD">GevSupportedOptionalCommandsTestDataReg</pVariable>
	  <pVariable Name="MT">GevSupportedOptionalCommandsManifestTableReg</pVariable>
	  <pVariable Name="CAP">GevSupportedOptionalCommandsCAPReg</pVariable>
	  <pVariable Name="LS">GevSupportedOptionalCommandsLSReg</pVariable>
	  <pVariable Name="HD">GevSupportedOptionalCommandsHDReg</pVariable>
	  <pVariable Name="SN">GevSupportedOptionalCommandsSNReg</pVariable>
	  <Formula>SEL=0?LLA:(SEL=1?DHCP:(SEL=2?PIP:(SEL=3?SCSS:(SEL=4?MCSS:(SEL=5?C:(SEL=6?W:(SEL=7?PR:(SEL=8?E:(SEL=9?ED:(SEL=10?P:(SEL=11?A:(SEL=12?ES:(SEL=13?DD:(SEL=14?WD:(SEL=15?TD:(SEL=16?MT:(SEL=17?CAP:(SEL=18?LS:(SEL=19?HD:(SEL=20?SN:0))))))))))))))))))))</Formula>
	</IntSwissKnife>

