			<Visibility>Expert</Visibility>
			<pValue>pSensorHeightReg</pValue>
		</Integer>
		<IntReg Name="pSensorHeightReg">
			<Address>0x20050</Address>
			<Length>4</Length>
			<AccessMode>RO</AccessMode>
			<pPort>Device</pPort>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

		<Enumeration Name="SensorTaps" NameSpace="Standard">
			<ToolTip>SensorTaps</ToolTip>
			<Description>SensorTaps</Description>
			<DisplayName>Sensor Taps</DisplayName>
			<Visibility>Expert</Visibility>
			<EnumEntry Name="Two" NameSpace="Standard">
				<ToolTip>Two</ToolTip>
				<Description>Two</Description>
				<DisplayName>Two</DisplayName>
				<Value>2</Value>
			</EnumEntry>
			<pValue>pSensorTapsReg</pValue>
		</Enumeration>
		<IntReg Name="pSensorTapsReg">
			<Address>0x20054</Address>
			<Length>4</Length>
			<AccessMode>RO</AccessMode>
			<pPort>Device</pPort>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

		<Enumeration Name="SensorDigitizationTaps" NameSpace="Standard">
			<ToolTip>SensorDigitizationTaps</ToolTip>
			<Description>SensorDigitizationTaps</Description>
			<DisplayName>Sensor Digitization Taps</DisplayName>
			<Visibility>Expert</Visibility>
			<EnumEntry Name="One" NameSpace="Standard">
				<ToolTip>One</ToolTip>
				<Description>One</Description>
				<DisplayName>One</DisplayName>
				<Value>2</Value>
			</EnumEntry>
			<pValue>pSensorDigitizationTapsReg</pValue>
		</Enumeration>
		<IntReg Name="pSensorDigitizationTapsReg">
			<Address>0x20058</Address>
			<Length>4</Length>
			<AccessMode>RO</AccessMode>
			<pPort>Device</pPort>
			<Cachable>WriteAround</Cachable>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

		<Integer Name="WidthMax" NameSpace="Standard">
			<ToolTip>Maximum width of the image in pixels</ToolTip>
			<Description>Maximum width of the image in pixels</Description>
			<DisplayName>Width Max</DisplayName>
			<Visibility>Expert</Visibility>
			<pValue>pWidthMaxReg</pValue>
		</Integer>
		<IntReg Name="pWidthMaxReg">
			<Address>0x2005C</Address>
			<Length>4</Length>
			<AccessMode>RO</AccessMode>
			<pPort>Device</pPort>
			<pInvalidator>pBinningHorizontalReg</pInvalidator>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

		<Integer Name="HeightMax" NameSpace="Standard">
			<ToolTip>Maximum height of the image in pixels</ToolTip>
			<Description>Maximum height of the image in pixels</Description>
			<DisplayName>Height Max</DisplayName>
			<Visibility>Expert</Visibility>
			<pValue>pHeightMaxReg</pValue>
		</Integer>
		<IntReg Name="pHeightMaxReg">
			<Address>0x20060</Address>
			<Length>4</Length>
			<AccessMode>RO</AccessMode>
			<pPort>Device</pPort>
			<pInvalidator>pBinningVerticalReg</pInvalidator>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

		<Integer Name="Width" NameSpace="Standard">
			<ToolTip>Width of the image in pixels</ToolTip>
			<Description>Width of the image in pixels</Description>
			<DisplayName>Width</DisplayName>
			<Visibility>Beginner</Visibility>
			<!--<pIsLocked>TLParamsLocked</pIsLocked>-->
			<pIsLocked>IsImageFormatLocked</pIsLocked>
			<Streamable>Yes</Streamable>
			<pValue>pWidthReg</pValue>
			<pMin>pWidthMin</pMin>
			<pMax>pWidthMax</pMax>
			<pInc>pWidthInc</pInc>
			<Representation>Linear</Representation>
		</Integer>
		<IntReg Name="pWidthReg">
			<Address>0x20064</Address>
			<Length>4</Length>
			<AccessMode>RW</AccessMode>
			<pPort>Device</pPort>
			<Cachable>WriteAround</Cachable>
			<pInvalidator>pLinePitchReg</pInvalidator>
	  <pInvalidator>pBinningHorizontalReg</pInvalidator>
	  <pInvalidator>pOffsetXReg</pInvalidator>
			<pInvalidator>pVideoSendModeSelectorReg</pInvalidator>
			<pInvalidator>pUserSetLoadReg</pInvalidator>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>
		<IntSwissKnife Name="pWidthMin">
			<pVariable Name="BINNINGH">pBinningHorizontalReg</pVariable>
			<Formula>BINNINGH*8</Formula>
		</IntSwissKnife>
		<IntSwissKnife Name="pWidthMax">
			<pVariable Name="MAXW">pWidthMaxReg</pVariable>
			<!--<pVariable Name="VOFFSETX">pOffsetXReg</pVariable>-->
			<!--<Formula>MAXW-VOFFSETX</Formula>-->
	  <Formula>MAXW-0</Formula>
		</IntSwissKnife>
		<IntSwissKnife Name="pWidthInc">
			<pVariable Name="BINNINGH">pBinningHorizontalReg</pVariable>
	  <Formula>(BINNINGH=1) ? BINNINGH*4 : BINNINGH*2</Formula>
		</IntSwissKnife>

		<Integer Name="Height" NameSpace="Standard">
			<ToolTip>Height of the image in pixels</ToolTip>
			<Description>Height of the image in pixels</Description>
			<DisplayName>Height</DisplayName>
			<Visibility>Beginner</Visibility>
			<!--<pIsLocked>TLParamsLocked</pIsLocked>-->
			<pIsLocked>IsImageFormatLocked</pIsLocked>
			<Streamable>Yes</Streamable>
			<pValue>pHeightReg</pValue>
			<Min>8</Min>
			<pMax>pHeightMax</pMax>
			<Inc>1</Inc>
			<!--<pInc>pBinningVerticalReg</pInc>-->
			<Representation>Linear</Representation>
		</Integer>
		<IntReg Name="pHeightReg">
			<Address>0x20068</Address>
			<Length>4</Length>
			<AccessMode>RW</AccessMode>
			<pPort>Device</pPort>
			<Cachable>WriteAround</Cachable>
			<pInvalidator>pBinningVerticalReg</pInvalidator>
			<pInvalidator>pOffsetYReg</pInvalidator>
			<pInvalidator>pVideoSendModeSelectorReg</pInvalidator>
			<pInvalidator>pUserSetLoadReg</pInvalidator>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>
		<IntSwissKnife Name="pHeightMax">
			<pVariable Name="MAXH">pHeightMaxReg</pVariable>
			<!--<pVariable Name="VOFFSETY">pOffsetYReg</pVariable>-->
			<!--<Formula>MAXH-VOFFSETY</Formula>-->
			<Formula>MAXH-0</Formula>
		</IntSwissKnife>

		<Integer Name="OffsetX" NameSpace="Standard">
			<ToolTip>X Offset of the image in pixels</ToolTip>
			<Description>X Offset of the image in pixels</Description>
			<DisplayName>Offset X</DisplayName>
			<Visibility>Beginner</Visibility>
			<!--<pIsLocked>TLParamsLocked</pIsLocked>-->
			<pIsLocked>IsImageFormatLocked</pIsLocked>
			<Streamable>Yes</Streamable>
			<pValue>pOffsetXReg</pValue>
			<Min>0</Min>
			<pMax>pOffsetXMax</pMax>
			<pInc>pWidthInc</pInc>
			<Representation>Linear</Representation>
		</Integer>
		<IntReg Name="pOffsetXReg">
			<Address>0x2006C</Address>
			<Length>4</Length>
			<AccessMode>RW</AccessMode>
			<pPort>Device</pPort>
			<Cachable>WriteAround</Cachable>
			<pInvalidator>pBinningHorizontalReg</pInvalidator>
			<pInvalidator>pWidthReg</pInvalidator>
			<pInvalidator>pVideoSendModeSelectorReg</pInvalidator>
			<pInvalidator>pUserSetLoadReg</pInvalidator>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>
		<IntSwissKnife Name="pOffsetXMax">
			<pVariable Name="MAXW">pWidthMaxReg</pVariable>
			<!--<pVariable Name="VWIDTH">pWidthReg</pVariable>-->
			<!--<Formula>MAXW-VWIDTH</Formula>-->
	  <Formula>MAXW-8</Formula>
		</IntSwissKnife>

		<Integer Name="OffsetY" NameSpace="Standard">
			<ToolTip>Y Offset of the image in pixels</ToolTip>
			<Description>Y Offset of the image in pixels</Description>
			<DisplayName>Offset Y</DisplayName>
			<Visibility>Beginner</Visibility>
			<!--<pIsLocked>TLParamsLocked</pIsLocked>-->
			<pIsLocked>IsImageFormatLocked</pIsLocked>
			<Streamable>Yes</Streamable>
			<pValue>pOffsetYReg</pValue>
			<Min>0</Min>
			<pMax>pOffsetYMax</pMax>
			<Inc>1</Inc>
			<!--<pInc>pBinningVerticalReg</pInc>-->
			<Representation>Linear</Representation>
		</Integer>
		<IntReg Name="pOffsetYReg">
			<Address>0x20070</Address>
			<Length>4</Length>
			<AccessMode>RW</AccessMode>
			<pPort>Device</pPort>
			<Cachable>WriteAround</Cachable>
			<pInvalidator>pBinningVerticalReg</pInvalidator>
			<pInvalidator>pHeightReg</pInvalidator>
			<pInvalidator>pVideoSendModeSelectorReg</pInvalidator>
			<pInvalidator>pUserSetLoadReg</pInvalidator>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>
		<IntSwissKnife Name="pOffsetYMax">
			<pVariable Name="MAXH">pHeightMaxReg</pVariable>
			<!--<pVariable Name="VHEIGHT">pHeightReg</pVariable>-->
			<!--<Formula>MAXH-VHEIGHT</Formula>-->
			<Formula>MAXH-8</Formula>
		</IntSwissKnife>

		<Integer Name="LinePitch" NameSpace="Standard">
