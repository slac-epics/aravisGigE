		  <pInvalidator>pUserSetLoadReg</pInvalidator>
		  <Sign>Unsigned</Sign>
		  <Endianess>BigEndian</Endianess>
		</IntReg>

		<Enumeration Name="DeviceTemperatureSelector" NameSpace="Standard">
			<ToolTip>DeviceTemperatureSelector</ToolTip>
			<Description>DeviceTemperatureSelector</Description>
			<DisplayName>Device Temperature Selector</DisplayName>
			<Visibility>Expert</Visibility>
			<EnumEntry Name="Mainboard" NameSpace="Standard">
				<ToolTip>Mainboard</ToolTip>
				<Description>Mainboard</Description>
				<DisplayName>Mainboard</DisplayName>
				<Value>1</Value>
			</EnumEntry>
			<pValue>pDeviceTemperatureSelectorReg</pValue>
			<pSelected>DeviceTemperature</pSelected>
		</Enumeration>
		<IntReg Name="pDeviceTemperatureSelectorReg">
			<Address>0x20028</Address>
			<Length>4</Length>
			<AccessMode>RW</AccessMode>
			<pPort>Device</pPort>
			<Cachable>WriteAround</Cachable>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

		<Float Name="DeviceTemperature" NameSpace="Standard">
			<ToolTip>DeviceTemperature</ToolTip>
			<Description>Device temperature in degrees Celsius(C).</Description>
			<DisplayName>Device Temperature</DisplayName>
			<Visibility>Expert</Visibility>
			<pValue>pDeviceTemperatureReg</pValue>
		</Float>
		<FloatReg Name="pDeviceTemperatureReg">
			<Address>0x2002C</Address>
			<Length>4</Length>
			<AccessMode>RO</AccessMode>
			<pPort>Device</pPort>
			<pInvalidator>pDeviceTemperatureSelectorReg</pInvalidator>
	  <pInvalidator>pUserSetLoadReg</pInvalidator>
			<Endianess>BigEndian</Endianess>
		</FloatReg>

		<Enumeration Name="DeviceClockSelector" NameSpace="Standard">
			<ToolTip>DeviceClockSelector</ToolTip>
			<Description>DeviceClockSelector</Description>
			<DisplayName>Device Clock Selector</DisplayName>
			<Visibility>Expert</Visibility>
			<EnumEntry Name="Sensor" NameSpace="Standard">
				<ToolTip>Sensor</ToolTip>
				<Description>Sensor</Description>
				<DisplayName>Sensor</DisplayName>
				<Value>0</Value>
			</EnumEntry>
			<!--<EnumEntry Name="SensorDigitization" NameSpace="Standard">
				<ToolTip>SensorDigitization</ToolTip>
				<Description>SensorDigitization</Description>
				<DisplayName>SensorDigitization</DisplayName>
				<Value>1</Value>
			</EnumEntry>-->
			<pValue>pDeviceClockSelectorReg</pValue>
			<pSelected>DeviceClockFrequency</pSelected>
		</Enumeration>
		<IntReg Name="pDeviceClockSelectorReg">
			<Address>0x20030</Address>
			<Length>4</Length>
			<AccessMode>RW</AccessMode>
			<pPort>Device</pPort>
			<Cachable>WriteAround</Cachable>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

		<Float Name="DeviceClockFrequency" NameSpace="Standard">
			<ToolTip>DeviceClockFrequency</ToolTip>
			<Description>DeviceClockFrequency</Description>
			<DisplayName>Device Clock Frequency</DisplayName>
			<Visibility>Expert</Visibility>
			<pValue>pDeviceClockFrequencyReg</pValue>
		</Float>
		<FloatReg Name="pDeviceClockFrequencyReg">
			<Address>0x20034</Address>
			<Length>4</Length>
			<AccessMode>RO</AccessMode>
			<pPort>Device</pPort>
			<pInvalidator>pDeviceClockSelectorReg</pInvalidator>
			<Endianess>BigEndian</Endianess>
		</FloatReg>

		<Command Name="DeviceReset" NameSpace="Standard">
			<ToolTip>Resets and reboots the device immediately.</ToolTip>
			<Description>Resets and reboots the device immediately.</Description>
			<DisplayName>Device Reset</DisplayName>
			<Visibility>Guru</Visibility>
			<pValue>pDeviceResetReg</pValue>
			<CommandValue>1</CommandValue>
		</Command>
		<IntReg Name="pDeviceResetReg">
			<Address>0x20038</Address>
			<Length>4</Length>
			<AccessMode>WO</AccessMode>
			<pPort>Device</pPort>
			<Cachable>NoCache</Cachable>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

	</Group>

