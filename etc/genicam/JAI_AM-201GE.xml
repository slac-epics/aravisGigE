	<Group Comment="AnalogControl">
		<Category Name="AnalogControl" NameSpace="Standard">
	  <ToolTip>Category that contains the Analog control features.</ToolTip>
	  <Description>This Category contains all the Analog control features.</Description>
			<DisplayName>Analog Control</DisplayName>
			<pFeature>GainSelector</pFeature>
			<pFeature>Gain</pFeature>
			<!--<pFeature>GainAbs</pFeature>-->
			<pFeature>GainRaw</pFeature>
			<pFeature>GainAuto</pFeature>
			<pFeature>GainAutoBalance</pFeature>
			<pFeature>BlackLevelSelector</pFeature>
			<pFeature>BlackLevelRaw</pFeature>
			<pFeature>BlackLevelAutoBalance</pFeature>
			<pFeature>BalanceRatioSelector</pFeature>
			<pFeature>BalanceRatio</pFeature>
			<!--<pFeature>BalanceRatioAbs</pFeature>-->
			<pFeature>BalanceWhiteAuto</pFeature>
			<pFeature>Gamma</pFeature>
			<pFeature>JAILUTMode</pFeature>
		</Category>

		<Enumeration Name="GainSelector" NameSpace="Standard">
	  <ToolTip>Selects which Gain is controlled by the various Gain features</ToolTip>
	  <Description>Selects which Gain is controlled by the various Gain features</Description>
			<DisplayName>Gain Selector</DisplayName>
			<Visibility>Beginner</Visibility>
			<Streamable>Yes</Streamable>
			<EnumEntry Name="AnalogAll" NameSpace="Standard">
		<ToolTip>Gain will be applied to all analog channels or taps.</ToolTip>
		<Description>Gain will be applied to all analog channels or taps.</Description>
				<DisplayName>Analog All</DisplayName>
				<Value>0</Value>
			</EnumEntry>
			<EnumEntry Name="DigitalAll" NameSpace="Standard">
		<ToolTip>Gain will be applied to all digital channels or taps.</ToolTip>
		<Description>Gain will be applied to all digital channels or taps.</Description>
				<DisplayName>Digital All</DisplayName>
		<Value>1</Value>
	  </EnumEntry>
	  <EnumEntry Name="DigitalRedAll" NameSpace="Standard">
		<ToolTip>Gain will be applied to the red digital channel.</ToolTip>
		<Description>Gain will be applied to the red digital channel.</Description>
		<DisplayName>Digital Red All</DisplayName>
		<pIsImplemented>IsBayerCamera</pIsImplemented>
		<Value>2</Value>
	  </EnumEntry>
	  <EnumEntry Name="DigitalBlueAll" NameSpace="Standard">
		<ToolTip>Gain will be applied to the blue digital channel.</ToolTip>
		<Description>Gain will be applied to the blue digital channel.</Description>
				<DisplayName>Digital Blue All</DisplayName>
		<pIsImplemented>IsBayerCamera</pIsImplemented>
		<Value>3</Value>
			</EnumEntry>
	  <EnumEntry Name="DigitalTap2All" NameSpace="Custom">
		<ToolTip>Gain will be applied to Tap2 digital channels.</ToolTip>
		<Description>Gain will be applied to Tap2 digital channels.</Description>
		<DisplayName>Digital Tap2 All</DisplayName>
		<Value>4</Value>
	  </EnumEntry>
	  <EnumEntry Name="DigitalTap2Red" NameSpace="Custom">
		<ToolTip>Gain will be applied to Tap2 red digital channels.</ToolTip>
		<Description>Gain will be applied to Tap2 red digital channels.</Description>
		<DisplayName>Digital Tap2 Red</DisplayName>
		<pIsImplemented>IsBayerCamera</pIsImplemented>
		<Value>5</Value>
	  </EnumEntry>
	  <EnumEntry Name="DigitalTap2Blue" NameSpace="Custom">
		<ToolTip>Gain will be applied to Tap2 blue digital channels.</ToolTip>
		<Description>Gain will be applied to Tap2 blue digital channels.</Description>
		<DisplayName>Digital Tap2 Blue</DisplayName>
		<pIsImplemented>IsBayerCamera</pIsImplemented>
		<Value>6</Value>
	  </EnumEntry>
 			<pValue>pGainSelectorReg</pValue>
			<pSelected>Gain</pSelected>
			<!--<pSelected>GainAbs</pSelected>-->
			<pSelected>GainRaw</pSelected>
			<pSelected>GainAuto</pSelected>
		</Enumeration>
		<IntReg Name="pGainSelectorReg">
			<Address>0x201F0</Address>
			<Length>4</Length>
			<AccessMode>RW</AccessMode>
			<pPort>Device</pPort>
	  <pInvalidator>pUserSetLoadReg</pInvalidator>
			<Sign>Unsigned</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

		<Float Name="Gain" NameSpace="Standard">
			<ToolTip>Controls the selected gain as an absolute physical value.</ToolTip>
			<Description>Controls the selected gain as an absolute physical value.</Description>
			<DisplayName>Gain</DisplayName>
			<Visibility>Beginner</Visibility>
			<Streamable>Yes</Streamable>
			<pValue>pGainReg</pValue>
	  <pMin>pGainMin</pMin>
	  <pMax>pGainMax</pMax>
	  <Representation>Linear</Representation>
  		<!--<Inc></Inc>-->
		</Float>
		<Float Name="GainAbs" NameSpace="Standard">
			<ToolTip>Controls the selected gain as an absolute physical value.</ToolTip>
			<Description>Controls the selected gain as an absolute physical value.</Description>
			<DisplayName>Gain Abs</DisplayName>
			<Visibility>Guru</Visibility>
			<Streamable>Yes</Streamable>
			<pValue>pGainReg</pValue>
	  <pMin>pGainMin</pMin>
	  <pMax>pGainMax</pMax>
	  <Representation>Linear</Representation>
			<!--<Inc></Inc>-->
		</Float>
		<FloatReg Name="pGainReg">
			<Address>0x201F4</Address>
			<Length>4</Length>
			<AccessMode>RW</AccessMode>
			<pPort>Device</pPort>
			<pInvalidator>pGainSelectorReg</pInvalidator>
			<pInvalidator>pGainRawReg</pInvalidator>
	  <pInvalidator>pBalanceRatioReg</pInvalidator>
	  <pInvalidator>pGainAutoReg</pInvalidator>
	  <pInvalidator>pUserSetLoadReg</pInvalidator>
	  <Endianess>BigEndian</Endianess>
	</FloatReg>

	<Integer Name="GainRaw" NameSpace="Standard">
	  <ToolTip>Controls the selected gain as a raw integer value.</ToolTip>
	  <Description>Controls the selected gain as a raw integer value.</Description>
	  <DisplayName>Gain Raw</DisplayName>
	  <Visibility>Guru</Visibility>
	  <Streamable>Yes</Streamable>
	  <pValue>pGainRawReg</pValue>
	  <pMin>pGainRawMin</pMin>
	  <pMax>pGainRawMax</pMax>
	  <Inc>1</Inc>
	  <Representation>Linear</Representation>
	</Integer>
	<IntReg Name="pGainRawReg">
	  <Address>0x201FC</Address>
	  <Length>4</Length>
	  <AccessMode>RW</AccessMode>
	  <pPort>Device</pPort>
	  <Cachable>WriteAround</Cachable>
	  <pInvalidator>pGainSelectorReg</pInvalidator>
	  <pInvalidator>pGainReg</pInvalidator>
	  <pInvalidator>pBalanceRatioReg</pInvalidator>
	  <pInvalidator>pGainAutoReg</pInvalidator>
	  <pInvalidator>pUserSetLoadReg</pInvalidator>
			<Sign>Signed</Sign>
			<Endianess>BigEndian</Endianess>
		</IntReg>

	<IntSwissKnife Name="pGainRawMin">
	  <pVariable Name="INDEX">pGainSelectorReg</pVariable>
	  <Formula>
		(INDEX=0) ? -84  : (
		(INDEX=1) ? -2393: (
		(INDEX=2) ? -4533: (
		(INDEX=3) ? -4533: (
		(INDEX=4) ? -891: (
		(INDEX=5) ? -891: (
		(INDEX=6) ? -891: -84 ))))))
	  </Formula>
	</IntSwissKnife>

	<IntSwissKnife Name="pGainRawMax">
	  <pVariable Name="INDEX">pGainSelectorReg</pVariable>
	  <Formula>
		(INDEX=0) ? 672 : (
		(INDEX=1) ? 3379: (
		(INDEX=2) ? 17713: (
		(INDEX=3) ? 17713: (
		(INDEX=4) ? 1000: (
		(INDEX=5) ? 1000: (
		(INDEX=6) ? 1000: 672 ))))))
	  </Formula>
	</IntSwissKnife>

	<SwissKnife Name="pGainMin">
	  <pVariable Name="INDEX">pGainSelectorReg</pVariable>
	  <Formula>
