	  <Formula>
		(INDEX=0) ? 16.0  : (
		(INDEX=1) ? 1.4125: (
		(INDEX=2) ? 3.1623: (
		(INDEX=3) ? 3.1623: (
		(INDEX=4) ? 1.1220: (
		(INDEX=5) ? 1.1220: (
